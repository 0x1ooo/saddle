{
  "name": "saddle",
  "productName": "Saddle",
  "version": "0.1.0",
  "description": "A Trojan GUI client",
  "main": "./.webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src --color",
    "format": "prettier --write",
    "test.patch": "node scripts/fix-jest-electron-bug.js",
    "test.main": "yarn test.patch && yarn jest \"src/(@common|main)\" --notify --config=jest.main.config.js --detectOpenHandles --passWithNoTests",
    "test.renderer": "yarn test.patch && yarn jest \"src/(@common|renderer)\" --notify --config=jest.renderer.config.js --detectOpenHandles --passWithNoTests",
    "test.all": "yarn test.main && yarn test.renderer"
  },
  "keywords": [],
  "author": {
    "name": "wizcas",
    "email": "o@0x1.ooo"
  },
  "license": "GPL-3.0-or-later",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test,mocks}/**/*.{json,css,scss,md}": [
      "prettier --write"
    ],
    "{src,test,mocks}/**/*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --ext .ts,.tsx,.js,.jsx --fix --color src"
    ]
  },
